<widget class="Dialog" name="OnX_HelpDialog_dialog">
  <geometry>700x400</geometry>
  <title>Help dialog</title>
  <show>no</show> 
  <widget class="VBox"> 
    <expand>first</expand>
    <sizeOfFixed>30</sizeOfFixed>
    <widget class="ScrolledText" name="OnX_HelpDialog_text"/>
    <widget class="PushButton">
      <label>Cancel</label>
      <activate exec="DLD">OnX ui_hide @shell@this@</activate>
    </widget>
  </widget>
</widget>

