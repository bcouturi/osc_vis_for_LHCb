./gui_in ${sogui}/SoGuiColorEditor.h.in ${from} ${to}/include/Inventor/Xt Xt SoXtColorEditor.h
./gui_in ${sogui}/editors/ColorEditor.h.in ${from} ${to}/include/Inventor/Xt/editors Xt SoXtColorEditor.h

# Template general :
./gui_in ${sogui}/SoAny.h.in ${from}  ${to}/include/Inventor/Xt Xt
./gui_in ${sogui}/SoAnyMaterialList.h.in ${from}  ${to}/include/Inventor/Xt Xt
