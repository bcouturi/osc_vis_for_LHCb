./gui_in ${sogui}/nodes/ColorEditor.h.in ${from} ${to}/include/Inventor/Xt/nodes Xt SoGuiColorEditor.h
./gui_in ${sogui}/nodes/MaterialEditor.h.in ${from} ${to}/include/Inventor/Xt/nodes Xt SoGuiMaterialEditor.h
./gui_in ${sogui}/nodes/ViewpointWrapper.h.in ${from} ${to}/include/Inventor/Xt/nodes Xt SoGuiViewpointWrapper.h

# Template engines :
./gui_in ${sogui}/engines/Engines.h.in ${from} ${to}/include/Inventor/Xt/engines Xt SoGuiEngines.h
./gui_in ${sogui}/engines/Format.h.in ${from} ${to}/include/Inventor/Xt/engines Xt SoGuiFormat.h
./gui_in ${sogui}/engines/RadioGroup.h.in ${from} ${to}/include/Inventor/Xt/engines Xt SoGuiRadioGroup.h

# Template editors :
./gui_in ${sogui}/SoGuiMaterialEditor.h.in ${from} ${to}/include/Inventor/Xt Xt SoXtMaterialEditor.h
