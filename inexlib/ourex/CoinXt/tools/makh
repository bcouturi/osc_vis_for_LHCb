./gui_in ${sogui}/nodes/Frame.h.in ${from} ${to}/include/Inventor/Xt/nodes Xt SoGuiFrame.h
./gui_in ${sogui}/nodes/Image.h.in ${from} ${to}/include/Inventor/Xt/nodes Xt SoGuiImage.h
./gui_in ${sogui}/nodes/Label.h.in ${from} ${to}/include/Inventor/Xt/nodes Xt SoGuiLabel.h
