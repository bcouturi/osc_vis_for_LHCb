./gui_in ${sogui}/SoGuiComponentP.h.in ${from}  ${to}/include/Inventor/Xt Xt SoGuiComponentP.h
./gui_in ${sogui}/SoGuiGLWidget.h.in ${from}  ${to}/include/Inventor/Xt Xt
#./gui_in ${sogui}/SoGuiMaterialList.h.in ${from}  ${to}/include/Inventor/Xt Xt
./gui_in ${sogui}/SoGuiObject.h.in ${from}  ${to}/include/Inventor/Xt Xt
./gui_in ${sogui}/SoGuiRenderArea.h.in ${from}  ${to}/include/Inventor/Xt Xt
./gui_in ${sogui}/SoGuiCursor.h.in ${from}  ${to}/include/Inventor/Xt Xt
./gui_in ${sogui}/gl.h.in ${from} ${to}/src/Inventor/Xt/common Xt
./gui_in ${sogui}/soguidefs.h.in ${from} ${to}/src Xt
./gui_in ${sogui}/SoGuiComponentP.h.in ${from}  ${to}/include/Inventor/Xt Xt SoGuiComponentP
./gui_in ${sogui}/SoGuiGLWidgetP.h.in ${from}  ${to}/include/Inventor/Xt Xt SoGuiGLWidgetP.h

./gui_in ${sogui}/SoGuiP.h.in ${from}  ${to}/include/Inventor/Xt Xt SoGuiP.h

# Not templated files :
find ${from}/src -name "Sb*.h" -exec ./cp_h {} ${from} ${to} \;
find ${from}/src -name "So*.h" -exec ./cp_h {} ${from} ${to} \;
find ${from}/src -name "Xt*.h" -exec ./cp_h {} ${from} ${to} \;
find ${from}/src -name "6DOFEvents.h" -exec ./cp_h {} ${from} ${to} \;
find ${from}/src -name "*.xpm" -exec ./cp_h {} ${from} ${to} \;

# Else :

/bin/rm -f ${to}/include/config.h

exit
